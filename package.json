{
  "private": true,
  "name": "very-short",
  "description": "",
  "license": "",
  "scripts": {
    "build": "concurrently \"npm:build:css\" \"npm:build:remix\"",
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "build:remix": "remix build",
    "dev": "concurrently \"npm:dev:css\" \"npm:dev:remix\"",
    "dev:css": "postcss styles --base styles --dir app/styles -w",
    "dev:remix": "remix dev",
    "start": "remix-serve build",
    "deploy": "flyctl deploy --build-arg REMIX_TOKEN=${REMIX_TOKEN}",
    "postinstall": "remix setup"
  },
  "dependencies": {
    "@prisma/client": "^3.4.0",
    "@remix-run/node": "^0.20.1",
    "@remix-run/react": "^0.20.1",
    "@remix-run/serve": "^0.20.1",
    "concurrently": "^6.2.1",
    "dotenv": "^10.0.0",
    "hashids": "^2.2.8",
    "prisma": "^3.4.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "6.0.0-beta.8",
    "remix": "^0.20.1",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^0.20.1",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.4",
    "@types/validator": "^13.6.6",
    "autoprefixer": "^10.3.1",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.2.7",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
