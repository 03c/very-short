{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:css && npm run build:remix",
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "build:remix": "remix build",
    "dev": "npm run dev:css && npm run dev:remix",
    "dev:css": "postcss styles --base styles --dir app/styles -w",
    "dev:remix": "remix dev",
    "start": "remix-serve build",
    "deploy": "flyctl deploy --build-arg REMIX_TOKEN=${REMIX_TOKEN}"
  },
  "dependencies": {
    "@remix-run/node": "^0.17.5",
    "@remix-run/react": "^0.17.5",
    "@remix-run/serve": "^0.17.5",
    "@supabase/supabase-js": "^1.21.3",
    "dotenv": "^10.0.0",
    "hashids": "^2.2.8",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^6.0.0-beta.0",
    "remix": "^0.17.5",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@remix-run/dev": "^0.17.5",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.4",
    "autoprefixer": "^10.3.1",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.2.7",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
